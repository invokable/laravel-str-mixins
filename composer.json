{
    "name": "revolution/laravel-str-mixins",
    "description": "Laravel Str mixins",
    "keywords": [
        "laravel",
        "mixins",
        "str"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "illuminate/support": "^11.0||^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\Laravel\\Mixins\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\Laravel\\Mixins\\StrMixinsServiceProvider"
            ]
        }
    }
}
